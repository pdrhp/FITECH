<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.calculadoraimc.TelaHistoricoDeCalculosActivity">

    <View
        android:id="@+id/frameHeaderCalculadoraIMCHistorico"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@drawable/btn1" />

    <TextView
        android:id="@+id/logoFITECH_CalculadoraIMCHistorico"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:fontFamily="@font/khand_bold"
        android:text="@string/app_name"
        android:textSize="40dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/line_1Historico"
        android:layout_width="68dp"
        android:layout_height="2dp"
        android:layout_marginLeft="-70dp"
        android:layout_marginTop="-18dp"
        android:background="@drawable/line_1"
        app:layout_constraintStart_toEndOf="@id/logoFITECH_CalculadoraIMCHistorico"
        app:layout_constraintTop_toBottomOf="@+id/logoFITECH_CalculadoraIMCHistorico" />

    <View
        android:id="@+id/iconButtonVoltarHistorico"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:background="@drawable/backwards_arrow"
        app:layout_constraintStart_toStartOf="@id/frameHeaderCalculadoraIMCHistorico"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="@id/frameHeaderCalculadoraIMCHistorico"
        android:layout_marginLeft="10dp"
        />

    <ListView
        android:id="@+id/listaHistoricoDeResultados"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@id/frameFooterCalculadoraIMCHistorico"
        app:layout_constraintTop_toBottomOf="@id/frameHeaderCalculadoraIMCHistorico" />

    <View
        android:id="@+id/frameFooterCalculadoraIMCHistorico"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:background="@drawable/btn1" />

</androidx.constraintlayout.widget.ConstraintLayout>